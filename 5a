#include<iostream>
using namespace std;
int mat[100][100]={0}, n, d[100]={0},ind=0;
void topo(int key){
 for(int i=0;i<n;i++){
 if(mat[key][i])
 {
 for(int j=0;j<n;j++)
 mat[j][i]=0;
 topo(i);
 }
 }
 d[ind++]=key;
}
int main(){
 cout<<"Enter the order of the matrix:";
 cin>>n;
 cout<<"Enter adjacent Matrix";
 for(int i=0;i<n;i++)
 for(int j=0;j<n;j++)
 cin>>mat[i][j];
 topo(0);
 for(int j=0;j<n;j++)
 cout<<d[j]<<endl;
}
